  "dependencies": {
    "d3-gauge": "~0.1.3"
  },
  "devDependencies": {
    "beefy": "~0.4.4",
    "tape": "~1.1.1"
  },
  "keywords": [],
  "author": {
    "name": "Thorsten Lorenz",
    "email": "thlorenz@gmx.de",
    "url": "http://thlorenz.com"
  },
  "license": {
    "type": "MIT",
    "url": "https://github.com/thlorenz/d3-gauge-writable/blob/master/LICENSE"
  },
  "engine": {
    "node": ">=0.6"
  },
